<form action="<%=cat_rental_requests_url%>" method="post">
  <label for="cat_rental_request_cat_id">Cat</label>
  <select name="cat_rental_request[cat_id]" id="cat_rental_request_cat_id">
    <option value="Select cat" disabled="true" selected>Select Cat</option>
    <% Cat.all.each do |cat| %>
      <option value="<%=cat.id%>"><%=cat.name%></option>
    <% end %>
  </select>
  <br>
  <label for="rental_start_date">start date</label>
  <input type="date" name="cat_rental_request[start_date]" value="">

  <br>

  <label for="rental_end_date">end date</label>
  <input type="date" name="cat_rental_request[end_date]" value="">

  <br>
  <input type="submit" value="Create Cat Rental">
</form>
